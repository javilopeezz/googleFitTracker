<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>
            {% block title %}
                Admin Dashboard

                            
            {% endblock %}
        </title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
                      
            <!-- Plantilla CSS -->
            <link rel="preconnect" href="https://fonts.gstatic.com">
            <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@300;400;600;700;800&display=swap" rel="stylesheet">

            <link rel="stylesheet" href=" {{ asset('plantillaAdmin/css/sb-admin-2.min.css') }}">
            <link rel="stylesheet" href=" {{ asset('plantillaAdmin/css/bootstrap.css') }} ">

            <link rel="stylesheet" href=" {{ asset('plantillaAdmin/vendors/iconly/bold.css') }}">

            <link rel="stylesheet" href=" {{ asset('plantillaAdmin/vendors/perfect-scrollbar/perfect-scrollbar.css') }}">
            <link rel="stylesheet" href=" {{ asset('plantillaAdmin/css/app.css') }}">
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <script src="https://kit.fontawesome.com/06d773c21c.js" crossorigin="anonymous"></script>
            
            <!-- Bootstrap core JavaScript-->
            <script src="{{ asset('plantillaAdmin/vendors/jquery/jquery.min.js') }}"></script>

            <script src="{{ asset('plantillaAdmin/vendors/perfect-scrollbar/perfect-scrollbar.min.js') }}"></script>
            <script src="{{ asset('plantillaAdmin/js/bootstrap.bundle.min.js') }}"></script>

            <script src="{{ asset('plantillaAdmin/vendors/apexcharts/apexcharts.js') }}"></script>
            <script src="{{ asset('plantillaAdmin/js/pages/dashboard.js') }}"></script>

            <script src="{{ asset('plantillaAdmin/js/main.js') }}"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
